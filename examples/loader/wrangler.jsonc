{
  "$schema": "../../node_modules/wrangler/config-schema.json",
  "name": "loader-demos",
  "main": "src/server.ts",
  "compatibility_date": "2026-01-28",
  "compatibility_flags": [
    "nodejs_compat",
    "experimental",
    "allow_irrevocable_stub_storage"
  ],
  "assets": {
    "directory": "public"
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "Think",
        "class_name": "Think"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": ["Think"]
    }
  ],
  "observability": {
    "enabled": true
  },
  "browser": {
    "binding": "BROWSER"
  },
  "worker_loaders": [
    {
      "binding": "LOADER"
    }
  ]
}
